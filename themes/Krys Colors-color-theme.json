			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		////////////////////////////////////////
		//// Special colored
		////////////////////////////////////////
		{	// TESTING SCOPE
			"scope": [
				"a"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF00D4"
				
			}
		},
