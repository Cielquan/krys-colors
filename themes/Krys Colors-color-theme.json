			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other",
			],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"punctuation.separator.dot.decimal",
			],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator",
				"punctuation.section",
				"punctuation.separator",
				"constant.language.import-export-all.js", // * in import
				"constant.language.import-export-all.ts", // * in import
