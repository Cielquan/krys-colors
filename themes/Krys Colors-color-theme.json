			"settings": {
				"foreground": "#FD971F"
			}
		},
		{
			"scope": [
				"markup.heading",
			],
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"scope": [
				"markup.bold",
			],
			"settings": {
