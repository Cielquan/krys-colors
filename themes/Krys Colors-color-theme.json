			"settings": {
				"fontStyle": "bold",
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Byte-String",
			"scope": [
