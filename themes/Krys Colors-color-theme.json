		////////////////////////////////////////
		////////////////////////////////////////
		////////////////////////////////////////
		//// GLOBAL DEFAULTS
		////////////////////////////////////////
		////////////////////////////////////////
		////////////////////////////////////////
		{
			"scope": [
				"source",
				"punctuation.separator",
				// "punctuation.separator.period",
				// "punctuation.separator.comma",
				// "punctuation.separator.arguments",
				// "punctuation.separator.parameter",
				// "punctuation.separator.parameters",
				// "punctuation.separator.element",
				// "punctuation.separator.list",
				// "punctuation.separator.mapping", // ',' in yaml mapping
				// "punctuation.separator.sequence", // ',' in yaml array
				// "punctuation.separator.array", // ',' in JSON array
				// "punctuation.separator.dictionary", // ':' ',' in JSON dict
				// "punctuation.separator.key-value.ini", // '='
				// "punctuation.separator.key-value.mapping.yaml", // ':'
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Constant",
