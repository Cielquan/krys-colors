			"settings": {
				"fontStyle": "",
				"foreground": "#94558D"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Function name",
			"scope": [
