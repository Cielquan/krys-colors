			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#859900"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Storage type",
			"scope": [
				"storage.type",
