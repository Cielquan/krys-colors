	"tokenColors": [
		////////////////////////////////////////
		////////////////////////////////////////
		////////////////////////////////////////
		//// GLOBAL DEFAULTS
		////////////////////////////////////////
		////////////////////////////////////////
		////////////////////////////////////////		
		{
			"scope": [
				"source",
				"punctuation.separator.period",
				"punctuation.separator.comma",
				"punctuation.separator.arguments",
				"punctuation.separator.parameter",
				"punctuation.separator.parameters",
				"punctuation.separator.element",
				"punctuation.separator.list",
			],
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"variable.other.constant",
				"constant.other.caps",
				"support.constant"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				// "punctuation.definition.string",
				"punctuation.definition.char",
			],
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "Byte-String",
			"scope": [
				"string.quoted.binary",
				"string.quoted.raw.binary",
			],
			"settings": {
				"foreground": "#A5C261"
			}
		},
		{
			"name": "Escapes",
			"scope": [
				"constant.character.escape",
				"constant.character.format",
				"punctuation.definition.interpolation",
