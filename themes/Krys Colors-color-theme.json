			"settings": {
				"foreground": "#6D6D94"
			}
		},
		{
			"scope": [
				"markup.underline.link",
				"meta.link",
			],
			"settings": {
				"foreground": "#E6DB74"
			}
		},	
		{
			"scope": [
				"markup.raw",
				"markup.inline.raw",
			],
			"settings": {
				"foreground": "#82A363"
