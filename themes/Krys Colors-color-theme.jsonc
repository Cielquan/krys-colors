      "scope": ["a"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF00D4"
      }
    },
    {
      "scope": [
        "keyword.operator.access.dot.rust", // .
        "string.regexp"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
