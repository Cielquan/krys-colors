        "string.quoted.triple.basic.block.toml", // triple "
        "string.quoted.single.basic.line.toml", // single "

        "string.unquoted.plain.in.yaml", // unquoted string in []
        "string.unquoted.plain.out.yaml", // unquoted string
        "string.unquoted.block.yaml", // unquoted string block
        "string.quoted.double.yaml", // "" string

        "string.quoted.double.json" // quoted string
      ],
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Config string (literal)",
      // TOML / YAML
      "scope": [
        "string.quoted.triple.literal.block.toml", // triple '
        "string.quoted.single.literal.line.toml", // single '
        "string.quoted.single.yaml" // '' string (literal)
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    ////////////////////////////////////////
    //// Log files
    ////////////////////////////////////////
    {
      "name": "Log Trace",
      "scope": ["log.trace"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Log Debug",
      "scope": ["log.debug", "log.verbose"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Log Info",
      "scope": ["log.info"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Log Warning",
      "scope": ["log.warning"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Log Error",
