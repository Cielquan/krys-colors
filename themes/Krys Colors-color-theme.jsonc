  "tokenColors": [
    ////////////////////////////////////////
    ////////////////////////////////////////
    ////////////////////////////////////////
    //// GLOBAL DEFAULTS
    ////////////////////////////////////////
    ////////////////////////////////////////
    ////////////////////////////////////////
    {
      "scope": [
        "source",
        "punctuation.separator"
        // "punctuation.separator.period",
        // "punctuation.separator.comma",
        // "punctuation.separator.arguments",
        // "punctuation.separator.parameter",
        // "punctuation.separator.parameters",
        // "punctuation.separator.element",
        // "punctuation.separator.list",
        // "punctuation.separator.mapping", // ',' in yaml mapping
        // "punctuation.separator.sequence", // ',' in yaml array
        // "punctuation.separator.array", // ',' in JSON array
        // "punctuation.separator.dictionary", // ':' ',' in JSON dict
        // "punctuation.separator.key-value.ini", // '='
        // "punctuation.separator.key-value.mapping.yaml", // ':'
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Constant",
      "scope": ["variable.other.constant", "constant.other.caps", "support.constant"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Byte-String",
      "scope": ["string.quoted.binary", "string.quoted.raw.binary"],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "Escapes",
      "scope": [
        "constant.character.escape",
        "constant.character.format",
        "punctuation.definition.interpolation",
        "meta.interpolation"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "punctuation.separator.dot.decimal"],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation.section",

        "punctuation.separator.key-value.html", // '=' (was yellow new red)
        "punctuation.separator.key-value.restructuredtext", // key value separator : (red)
        "punctuation.separator.key-value.markdown", // red

        "punctuation.separator.annotation.python", // ':' before type annotation in def
        "punctuation.separator.annotation.result.python", // '->' before return type annotation in def
        "punctuation.separator.colon.python", // ':' after type annotions in body
        "punctuation.separator.dict.python", // ':' in dicts
        "punctuation.separator.slice.python", // slice operator ':'
        "punctuation.separator.statement.and.shell.dockerfile", // &&

        "constant.language.import-export-all.js", // * in import
        "constant.language.import-export-all.jsx", // * in import
        "constant.language.import-export-all.ts", // * in import
        "constant.language.import-export-all.tsx", // * in import
        "storage.type.function.arrow.js", // =>
        "storage.type.function.arrow.jsx", // =>
        "storage.type.function.arrow.ts", // =>
        "storage.type.function.arrow.tsx", // =>
        "keyword.other.unit.percentage.css", // %
        "punctuation.definition.directive.begin.yaml", // %
        "constant.language.merge.yaml", // <<
        "punctuation.definition.lifetime.rust", // '
        "punctuation.definition.decorator.python" // @
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Operator (logical)",
      "scope": ["keyword.operator.logical"],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword.other"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Keyword (control)",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Keyword (import)",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Keyword (special)",
      "scope": [
        "storage.type.format.python",
        "storage.type.function.async.python",
        "keyword.other.important.css",
        "keyword.operator.new.js",
        "keyword.operator.new.jsx",
        "keyword.operator.new.ts",
        "keyword.operator.new.tsx",
        "meta.other.directive.restructuredtext",
        "entity.name.role.restructuredtext"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#859900"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "fontStyle": "",
        "foreground": "#F92672"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Storage type (special)",
      "scope": [
        "storage.type.string",
        "string.quoted.byte.raw.rust", // b and r in front of string or char
        "punctuation.definition.string.raw.rust", // # for raw strings
        "storage.type.number",
        "storage.type.imaginary.number"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Storage type (class/func)",
      "scope": ["storage.type.class", "storage.type.function", "keyword.other.fn"],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class",
        "entity.name.type.numeric",
        "entity.name.type.primitive",
        "entity.name.type.option",
        "entity.name.type.result"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.enum",
        "entity.name.type.struct",
        "entity.name.type.trait",
        "entity.name.type.declaration"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Self",
      "scope": ["variable.language", "variable.parameter.function.language.special"],
      "settings": {
        "fontStyle": "",
        "foreground": "#94558D"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "variable.other.jinja.filter"],
      "settings": {
        "fontStyle": "",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Namespace/Scope",
      "scope": ["entity.name.namespace", "entity.name.scope-resolution"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A7FF95"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "fontStyle": "",
        "foreground": "#F92672"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "",
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "Comment (documentation)",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#629755"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "keyword.illegal"],
      "settings": {
        "fontStyle": "bold italic underline",
        "foreground": "#F92672"
      }
    },
    {
      "scope": ["token.info-token"],
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": ["token.warn-token"],
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": ["token.error-token"],
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": ["token.debug-token"],
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "diff.inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": ["string.other.link", "markup.underline"],
      "settings": {
        "foreground": "#FD971F"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#66D9EF"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66D9EF"
      }
    },
    {
      "scope": ["markup.quote", "markup.other.command", "string.other.interpreted"],
      "settings": {
        "foreground": "#6D6D94"
      }
    },
    {
      "scope": ["markup.underline.link", "meta.link"],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": {
        "foreground": "#82A363"
      }
    },
    {
      "scope": [
        "markup.raw.inner",
        "markup.raw.block",
        "markup.fenced_code",
        "punctuation.section.raw"
      ],
      "settings": {
        "foreground": "#4F7E4E"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    ////////////////////////////////////////
    //// Config files
    ////////////////////////////////////////
    {
      "name": "Config section",
      // INI / TOML
      "scope": ["entity.name.section.group-title", "variable.key.table"],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Config array",
      // TOML
      "scope": ["variable.key.array"],
      "settings": {
        "foreground": "#2FA9FF"
      }
    },
    {
      "name": "Config key",
      // INI / TOML / YAML / JSON
      "scope": [
        "keyword.other.definition.ini",
        "keyword.key.toml",
        "variable.key.toml",
        "entity.name.tag.yaml",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Config built-in constant",
      // TOML / YAML / JSON
      "scope": [
        "constant.other.boolean.toml",
        "constant.language.boolean.toml",
        "constant.language.boolean.yaml",
        "constant.language.json"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Config date",
      // TOML / YAML
      "scope": [
        "constant.other.datetime-with-timezone.toml",
        "constant.other.datetime.toml",
        "constant.other.date.toml",
        "constant.other.time.datetime.offset.toml",
        "constant.other.time.datetime.local.toml",
        "constant.other.time.date.toml",
        "constant.other.time.time.toml",
        "constant.other.timestamp.yaml"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Config string",
      // INI / TOML / YAML / JSON
      "scope": [
        "string.quoted.double.ini", // quoted string double
        "string.quoted.single.ini", // quoted string single
